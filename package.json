{
  "name": "costco-special-offer",
  "version": "1.0.0",
  "packageManager": "yarn@4.0.2",
  "description": "A simple Discord bot for Costco special offer notifications",
  "main": "costco.ts",
  "type": "module",
  "scripts": {
    "start": "node main.js -- cron",
    "start:onetime": "node main.js -- onetime",
    "start:onetime:all": "node main.js -- onetime:all",
    "pm2": "pm2 start yarn --name costco-special-offer -- start"
  },
  "author": "Jaeyong",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "discord-interactions": "^3.2.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.19.8",
    "node-cron": "^3.0.3",
    "nodemon": "^3.0.2"
  }
}
